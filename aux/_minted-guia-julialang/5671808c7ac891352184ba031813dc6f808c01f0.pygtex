  \PYG{n}{g} \PYG{o}{=} \PYG{n}{mod}\PYG{p}{(}\PYG{n}{y}\PYG{p}{,} \PYG{l+m+mi}{100}\PYG{p}{)}
  \PYG{c}{\PYGZsh{} Definir e y f en función del mes (de 1 a 12) y el siglo}
  \PYG{c}{\PYGZsh{} (en ciclos de 400 años \PYGZhy{}\PYGZhy{}\PYGZhy{} 4 siglos)}
  \PYG{n}{earray} \PYG{o}{=} \PYG{p}{[}\PYG{l+m+mi}{0}\PYG{p}{,}\PYG{l+m+mi}{3}\PYG{p}{,}\PYG{l+m+mi}{2}\PYG{p}{,}\PYG{l+m+mi}{5}\PYG{p}{,}\PYG{l+m+mi}{0}\PYG{p}{,}\PYG{l+m+mi}{3}\PYG{p}{,}\PYG{l+m+mi}{5}\PYG{p}{,}\PYG{l+m+mi}{1}\PYG{p}{,}\PYG{l+m+mi}{4}\PYG{p}{,}\PYG{l+m+mi}{6}\PYG{p}{,}\PYG{l+m+mi}{2}\PYG{p}{,}\PYG{l+m+mi}{4}\PYG{p}{]}
  \PYG{n}{farray} \PYG{o}{=} \PYG{p}{[}\PYG{l+m+mi}{0}\PYG{p}{,}\PYG{l+m+mi}{5}\PYG{p}{,}\PYG{l+m+mi}{3}\PYG{p}{,}\PYG{l+m+mi}{1}\PYG{p}{]}
