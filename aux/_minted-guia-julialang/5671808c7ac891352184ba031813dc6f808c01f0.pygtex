  \PYG{n}{f} \PYG{o}{=} \PYG{n}{farray}\PYG{p}{[}\PYG{n}{mod}\PYG{p}{(}\PYG{n}{c}\PYG{p}{,}\PYG{l+m+mi}{4}\PYG{p}{)}\PYG{o}{+}\PYG{l+m+mi}{1}\PYG{p}{]}
  \PYG{c}{\PYGZsh{} Seleccionar el día de la semana en función del cálculo de Gauss}
  \PYG{n}{warray} \PYG{o}{=} \PYG{p}{[}\PYG{l+s}{\PYGZdq{}domingo\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}lunes\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}martes\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}miércoles\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}jueves\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}viernes\PYGZdq{}}\PYG{p}{,}\PYG{l+s}{\PYGZdq{}sábado\PYGZdq{}}\PYG{p}{]}
  \PYG{n}{w} \PYG{o}{=} \PYG{n}{mod}\PYG{p}{(}\PYG{n}{d} \PYG{o}{+} \PYG{n}{e} \PYG{o}{+} \PYG{n}{f} \PYG{o}{+} \PYG{n}{g} \PYG{o}{+} \PYG{n}{div}\PYG{p}{(}\PYG{n}{g}\PYG{p}{,} \PYG{l+m+mi}{4}\PYG{p}{),} \PYG{l+m+mi}{7}\PYG{p}{)}
  \PYG{k}{return}\PYG{p}{(}\PYG{n}{warray}\PYG{p}{[}\PYG{n}{w}\PYG{o}{+}\PYG{l+m+mi}{1}\PYG{p}{])}
\PYG{k}{end}
